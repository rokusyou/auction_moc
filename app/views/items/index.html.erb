<h1>Items#index</h1>
<p>Find me in app/views/items/index.html.erb</p>
<table>
  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.price %></td>
        <td><%= item.description %></td>
        <td><%= link_to "Show", item %></td>
      </tr>
    <% end %>
  </tbody>
</table>
